.container
  .result
  .panel-title
    -# 本情報取得
    
  = form_with(model: article, local: true) do |f|
    %div
      = f.hidden_field :book_title, :value => book_title
    %div
      = f.hidden_field :image_url, :value => image_url
    %div
      = f.label :title, 'タイトル'
    %div
      = f.text_field :title
    %div
      = f.label :content, '内容'
    %div
      = f.text_area :content
    = f.submit '保存'